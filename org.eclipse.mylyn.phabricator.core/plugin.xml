<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.mylyn.phabricator.repository"
         name="%repository.name"
         point="org.eclipse.mylyn.tasks.ui.repositories">
      <connectorCore
            class="org.eclipse.mylyn.internal.phabricator.core.PhabricatorRepositoryConnector"
            id="phabricator"
            name="%TracRepositoryConnector.name"/>
      <taskListMigrator
            class="org.eclipse.mylyn.internal.phabricator.core.TracTaskListMigrator">
      </taskListMigrator>
      <repositoryMigrator
            class="org.eclipse.mylyn.internal.phabricator.core.TracRepositoryMigrator">
      </repositoryMigrator>
   </extension>
   <extension
         point="org.eclipse.mylyn.tasks.core.templates">
      <repository
            addAutomatically="false"
            anonymous="false"
            label="S02"
            repositoryKind="phabricator"
            urlRepository="http://s02.labs.h3.se"
            version="XXX"/>
   </extension>
      <extension
         point="org.eclipse.mylyn.tasks.core.templates">
      <repository
            addAutomatically="false"
            anonymous="false"
            label="ZZZ"
            repositoryKind="phabricator"
            urlRepository="http://s02.labs.h3.se"
            version="XXX"/>
   </extension>  
</plugin>
